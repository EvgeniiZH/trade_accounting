{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npx vite@latest --host --config vite.config.mjs",
    "start": "npx vite@latest --host --config vite.config.mjs",
    "build": "npx vite@latest build --config vite.config.mjs",
